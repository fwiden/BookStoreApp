{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information"
            }
        },
        "WriteTo": [
            {
                "Args": {
                    "path": "./logs/log-.txt",
                    "rollingInterval": "Day"
                },
                "Name": "File"
            }
        ]
    },

    
    "AllowedHosts": "*"
}
